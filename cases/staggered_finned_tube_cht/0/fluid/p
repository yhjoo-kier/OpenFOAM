FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      p;
}

dimensions      [1 -1 -2 0 0 0 0];
internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            cyclicAMI;
        neighbourPatch  outlet;
        transform       translational;
        separationVector (0.06 0 0);
    }
    outlet
    {
        type            cyclicAMI;
        neighbourPatch  inlet;
        transform       translational;
        separationVector (-0.06 0 0);
    }
    top
    {
        type            cyclicAMI;
        neighbourPatch  bottom;
        transform       translational;
        separationVector (0 -0.06 0);
    }
    bottom
    {
        type            cyclicAMI;
        neighbourPatch  top;
        transform       translational;
        separationVector (0 0.06 0);
    }
    front
    {
        type            cyclicAMI;
        neighbourPatch  back;
        transform       translational;
        separationVector (0 0 -0.05);
    }
    back
    {
        type            cyclicAMI;
        neighbourPatch  front;
        transform       translational;
        separationVector (0 0 0.05);
    }
    fluid_to_solid
    {
        type            fixedFluxPressure;
    }
}
