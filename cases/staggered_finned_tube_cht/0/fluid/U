FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/fluid";
    object      U;
}

dimensions      [0 1 -1 0 0 0 0];
internalField   uniform (1.0 0 0);

boundaryField
{
    inlet
    {
        type            cyclicAMI;
        neighbourPatch  outlet;
        transform       translational;
        separationVector (0.12 0 0);
    }
    outlet
    {
        type            cyclicAMI;
        neighbourPatch  inlet;
        transform       translational;
        separationVector (-0.12 0 0);
    }
    top
    {
        type            symmetry;
    }
    bottom
    {
        type            symmetry;
    }
    front
    {
        type            symmetry;
    }
    back
    {
        type            symmetry;
    }
fluid_to_domain0
{
    type            noSlip;
}
fluid_to_domain1
{
    type            noSlip;
}
fluid_to_solid
{
    type            noSlip;
}
}
