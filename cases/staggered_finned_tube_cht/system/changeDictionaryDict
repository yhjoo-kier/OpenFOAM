FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      changeDictionaryDict;
}

dictionaryReplacement
{
    boundary
    {
        inlet
        {
            type            cyclicAMI;
            neighbourPatch  outlet;
            transform       translational;
            separationVector (0.12 0 0);  // 2*S_L
            matchTolerance  0.1;
        }

        outlet
        {
            type            cyclicAMI;
            neighbourPatch  inlet;
            transform       translational;
            separationVector (-0.12 0 0);  // -2*S_L
            matchTolerance  0.1;
        }

        top
        {
            type            symmetry;
        }

        bottom
        {
            type            symmetry;
        }

        front
        {
            type            symmetry;
        }

        back
        {
            type            symmetry;
        }
    }
}
